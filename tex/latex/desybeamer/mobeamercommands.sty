\ProvidesPackage{mobeamercommands}[Custom Commands to safe time]
% Custom commands for just including images
\newcommand{\icg}[1]{\begin{center}\includegraphics[keepaspectratio,width=.9\textwidth,height=.8\textheight]{#1}\end{center}}
\newcommand{\icgsmall}[1]{\begin{center}\includegraphics[keepaspectratio,width=.9\textwidth,height=.6\textheight]{#1}\end{center}}

\newcommand{\NN}[1]{\mathrm{NN}_\mathrm{#1}\xspace}






\newcommand{\imgframe}[1]{%
	{ % all template changes are local to this group.
			\setbeamertemplate{navigation symbols}{}
			\begin{frame}<article:0>[plain]
				% \begin{tikzpicture}[remember picture,overlay]
				% 	\node[at=(current page.center)] {
				% 		\includegraphics[keepaspectratio,
				% 			width=\textwidth,
				% 			height=\textheight]{#1}
				% 	};
				% \end{tikzpicture}
				\centering
				\includegraphics[keepaspectratio,width=0.95\textwidth,height=0.95\textheight]{#1}
			\end{frame}
		}
}


